<h1 id="title">Listing break_points</h1>

<table>
  <tr>
    <th>City</th>
    <th>Province</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @break_points.each do |break_point| %>
  <tr>
    <td><%= break_point.city %></td>
    <td><%= break_point.province.name %></td>
    <td><%= link_to 'Show', break_point %></td>
    <td><%= link_to 'Edit', edit_break_point_path(break_point) %></td>
    <td><%= link_to 'Destroy', break_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<div class="links">
 <%= link_to 'New Break point', new_break_point_path %>
 &nbsp;&nbsp;
 <%= link_to 'Index', home_index_path %>
</div>
